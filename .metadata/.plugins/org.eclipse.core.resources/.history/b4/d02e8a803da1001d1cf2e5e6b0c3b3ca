package test.employee;

public class Employee {
	String no;
	String name;
	String phone;
	int pay;
	
	/**
	 * @param no
	 * @param name
	 * @param phone
	 * @param pay
	 */
	public Employee(String no, String name, String phone, int pay) {
		super();
		this.no = no;
		this.name = name;
		this.phone = phone;
		this.pay = pay;
	}

	public int getPay() {return 0;}
}
