package test.employee;

public class Engineer extends Employee{
	private String dept;
	
	/**
	 * @param no
	 * @param name
	 * @param phone
	 * @param pay
	 * @param dept
	 */
	public Engineer(String no, String name, String phone, int pay, String dept) {
		super(no, name, phone, pay);
		this.dept = dept;
	}

	@Override
	public int getPay()
	{
		System.out.println(super.getPay());
		return 0;
	}
}
