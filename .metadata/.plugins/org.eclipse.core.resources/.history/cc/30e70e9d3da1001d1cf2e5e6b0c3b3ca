package test.employee;

public class Employee {
	private String no;
	private String name;
	private String phone;
	private int pay;
	
	/**
	 * @param no
	 * @param name
	 * @param phone
	 * @param pay
	 */
	public Employee(String no, String name, String phone, int pay) {
		super();
		this.no = no;
		this.name = name;
		this.phone = phone;
		this.pay = pay;
	}

	public int getPay() {return this.pay;}
}
