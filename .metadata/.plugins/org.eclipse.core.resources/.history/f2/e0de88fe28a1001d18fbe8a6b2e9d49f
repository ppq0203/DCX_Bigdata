package problem;
/**
 * 다음과 같은 결과를 출력하세요
 */
public class Problem22 {

	public static void main(String[] args) {
		for(int i = 0; i < 5; ++i)
		{
			for(int j = 0; j < 5; ++j)
			{
				if( (i+j) < 4)
				{
					System.out.print("  ");
				}
				else
				{
					System.out.print("*");					
				}
			}
			System.out.println();
		}
	}

}
