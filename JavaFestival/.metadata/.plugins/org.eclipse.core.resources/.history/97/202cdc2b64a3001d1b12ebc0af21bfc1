package festival.algorithm;

import java.util.HashSet;
import java.util.Set;

public class Prob18 {

	public static void main(String[] args) {
		Set<Integer> set = new HashSet<Integer>();
		while(set.size()<6)
		{
			set.add((int) (Math.random()*6));
		}
		for(int num : set)
		{
			System.out.println("행운의숫자 : " + num);
		}
	}

}
